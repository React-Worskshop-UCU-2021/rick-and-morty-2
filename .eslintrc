{
  "root": true,
  //"parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:import/warnings", "plugin:import/errors"],
  "plugins": ["react", "import"],
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-multi-assign": 1,
    "semi": [2, "always"],
    "import/order": 2,
    "react/jsx-key": 2,
    "react/default-props-match-prop-types": 2,
    "react/prop-types": 2,
    "react/react-in-jsx-scope": "off"
  }
}
